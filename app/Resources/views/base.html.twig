<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}Coleta Seletiva{% endblock %}</title>
        {% stylesheets filter="cssrewrite"
            'css/bootstrap.css'
            'components/bootstrap-datepicker/css/datepicker3.css'
            'components/jquery-ui/themes/base/jquery-ui.css'
            'components/select2/select2.css'
            'components/select2/select2-bootstrap.css'
            'bundles/enficienciasdoambientesite/css/style.css' %}
            <link href="{{ asset_url }}" rel="stylesheet" media="screen">
        {% endstylesheets %}
        {% stylesheets filter="cssrewrite"
        %}
            <link href="{{ asset_url }}" rel="stylesheet" media="print">
        {% endstylesheets %}

        <!-- HTML5 Shim and Respond.js add IE8 support of HTML5 elements and media queries -->
        {% include 'BraincraftedBootstrapBundle::ie8-support.html.twig' %}

        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        {% block head %}{% endblock head %}
    </head>
    <body>
        {% block body %}{% endblock body %}
        {% javascripts
            'js/jquery.js'
            'components/jquery-ui/ui/jquery-ui.js'
            'components_manual/jquery-easy-confirm/jquery.easy-confirm-dialog.js'
            'js/bootstrap.js'
            'components/bootstrap-datepicker/bootstrap-datepicker-built.js'
            'components/select2/select2.js'
            'components/select2/select2_locale_pt-BR.js'
            'components_manual/bootbox/bootbox.js'
            %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% block javascripts %}{% endblock %}
    </body>
</html>

