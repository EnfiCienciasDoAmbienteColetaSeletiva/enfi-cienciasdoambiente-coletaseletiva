{% extends '::base.html.twig' %}

{% block body -%}
    <h1>PontoDeColeta list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Idfixo</th>
                <th>Revision</th>
                <th>Ativo</th>
                <th>Endereco</th>
                <th>Telefone</th>
                <th>Horariodefuncionamento</th>
                <th>Latitude</th>
                <th>Longitude</th>
                <th>Usuarioid</th>
                <th>Timestamp</th>
                <th>Descricaodasmodificacoes</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('manage_pontodecoleta_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.idFixo }}</td>
                <td>{{ entity.revision }}</td>
                <td>{{ entity.ativo }}</td>
                <td>{{ entity.endereco }}</td>
                <td>{{ entity.telefone }}</td>
                <td>{{ entity.horarioDeFuncionamento }}</td>
                <td>{{ entity.latitude }}</td>
                <td>{{ entity.longitude }}</td>
                <td>{{ entity.usuarioId }}</td>
                <td>{{ entity.timestamp }}</td>
                <td>{{ entity.descricaoDasModificacoes }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('manage_pontodecoleta_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('manage_pontodecoleta_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('manage_pontodecoleta_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
